#include <stdlib.h>
#include <curses.h>
#include <signal.h>
#include "sk_manager.h"

/* 
 * Entry point to text editor program.
 */

void main (int argc, char * argv[])
{
	int command;
	int x;
	int y;
	
	init();
	printw("EdIt v1.0 - Ctrl + C to close...\n");
	refresh();
	
	for(;;)
	{
		command = getch();
		getyx(stdscr, y, x);
		refresh();
		get_command(command, y, x);		
	}
}
